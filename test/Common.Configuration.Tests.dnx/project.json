{
  "webroot": "wwwroot",
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": false
  },
  "dependencies": {
    "Microsoft.AspNet.Hosting": "1.0.0-rc1-final",
    "Microsoft.AspNet.Server.WebListener": "1.0.0-rc1-final",
    "Common.Owin.vnext": { "target": "project" },
    "Common.Configuration": { "target": "project" },
    "Common.Utils.log4net": { "target": "project" },
    "Common.Configuration.log4net": { "target": "project" },
    "Microsoft.AspNet.Diagnostics": "1.0.0-rc1-final",
    "Microsoft.AspNet.IISPlatformHandler": "1.0.0-rc1-final",
    "Microsoft.AspNet.Server.Kestrel": "1.0.0-rc1-final",
    "Microsoft.Extensions.PlatformAbstractions": "1.0.0-rc1-final",
    "xunit": "2.1.0",
    "xunit.runner.dnx": "2.1.0-rc1-build204",
    "Should": "1.1.20",
    "Holycode.Performance": { "target": "project" },
    "Holycode.Performance.NewRelic": { "target": "project" }

  },
  "//dependencies": {
    "dotnet-test-xunit": "1.0.0-rc2-build10025"

  },

  "commands": {
    "web": "Microsoft.AspNet.Server.Kestrel",
    "xunit": "xunit.runner.dnx",
    "run": "Common.Configuration.Tests.dnx",
    "test": "Common.Configuration.Tests.dnx"

  },

  "frameworks": {
    "dnx451": {
      "frameworkAssemblies": {
        "System.Configuration": "4.0.0.0",
        "System.Collections": "4.0.0.0"
      }
    }
  },
  "//frameworks": {
    "netcoreapp1.0": {
      "imports": "dnx451"
    }

  },
  "publishExclude": [
    "node_modules",
    "bower_components",
    "**.xproj",
    "**.user",
    "**.vspscc"
  ],
  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ]
}
